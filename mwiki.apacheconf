# -*- mode: conf; -*-
Alias /mwiki "/var/cache/mwiki/public/compiled/html"
<Directory /mwiki>
  Options +Indexes
  DirectoryIndex 00INDEX.html
  Order allow,deny
  allow from all
</Directory>

ScriptAlias /cgi-bin/mwiki/ /usr/lib/cgi-bin/mwiki
<Directory "/usr/lib/cgi-bin/mwiki">
  AllowOverride None
  Options +ExecCGI
  SetHandler cgi-script
  Order allow,deny
  Allow from all
</Directory>
