AliasMatch ^/##REPO_NAME##/([^\/]*)([\/].*)? "##REPOS_BASE##/$1/html$2"

<Directory /##REPO_NAME##/*>
  Options Indexes FollowSymLinks MultiViews
  DirectoryIndex ##REPO_DIR_INDEX##
  AllowOverride None
  Order allow,deny
  allow from all
</Directory>

<Directory /var/www/bin/>
        AllowOverride None
        Options ExecCGI SymLinksIfOwnerMatch
        AddHandler cgi-script .cgi .pl
        Order allow,deny
        Allow from all
</Directory>

SuexecUserGroup ##MWUSER## ##MWUSER##
